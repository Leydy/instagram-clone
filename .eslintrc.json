{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules":{
        "react/jsx-filename-extension":0,
        "no-use-before-define": 0,
        "linebreak-style":0,
        "react/prop-types":0,
        "react/prefer-stateless-function":0,
        "object-curly-newline":0,
        "object-shorthand":0,
        "semi":0,
        "comma-dangle":0,
        "arrow-body-style":0,
        "consistent-return":0,
        "react/sort-comp":0,
        "react/jsx-wrap-multilines":0,
        "react/destructuring-assignment":0
    }
}